<table>
  <tr>
    <th>id</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone</th>
    <th>Email</th>
  </tr>
  {{#each}}
    <tr class="client-row">
      <td>{{#link-to 'clients.show' this}}{{id}}{{/link-to}}</td>
      <td>{{firstName}}</td>
      <td>{{lastName}}</td>
      <td>{{phone}}</td>
      <td>{{email}}</td>
    </tr>
  {{/each}}
</table>
